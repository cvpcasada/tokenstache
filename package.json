{
  "name": "iter-stache",
  "version": "1.0.0",
  "description": "tokenizes a string with mustache variables",
  "source": "tokenstache.js",
  "module": "dist/tokenstache.es.js",
  "main": "dist/tokenstache.js",
  "umd:main": "dist/tokenstache.umd.js",
  "repository": "https://github.com/cvpcasada/tokenstache",
  "author": "Cyrus Venn Casada",
  "license": "MIT",
  "keywords": [
    "mustache",
    "handlebars",
    "tokenizer"
  ],
  "scripts": {
    "build": "rollup -c --environment FORMAT:umd && rollup -c --environment FORMAT:cjs && rollup -c --environment FORMAT:es",
    "test": "jest",
    "prepare": "npm run build",
    "release": "npm run -s build && git commit -am $npm_package_version && git tag $npm_package_version && git push && git push --tags && npm publish"
  },
  "devDependencies": {
    "babel-jest": "^21.2.0",
    "babel-preset-env": "^1.6.1",
    "jest": "^21.2.1",
    "prettier": "^1.9.1",
    "rollup": "^0.52.1",
    "rollup-plugin-buble": "^0.18.0",
    "rollup-plugin-es3": "^1.1.0",
    "rollup-plugin-post-replace": "^1.0.0",
    "rollup-plugin-uglify": "^2.0.1"
  },
  "dependencies": {},
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": true
          }
        }
      ]
    ]
  },
  "files": [
    "tokenstache.js",
    "dist"
  ]
}
